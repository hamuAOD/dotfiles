{
  "title": "My Complex",
  "rules": [
    {
      "description": "Remap Caps Lock to Ctrl when held, Esc when tapped",
      "manipulators": [
        {
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_control",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "escape"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Spacebar + hjkkl to Arrow-Key",
      "manipulators": [
        {
          "type": "basic",
          "description": "Spacebar + h to left arrow",
          "conditions": [ { "name": "spacebar-flag", "type": "variable_if", "value": 1 } ],
          "from": { "key_code": "h" },
          "to": [ { "key_code": "left_arrow" } ]
        },
        {
          "type": "basic",
          "description": "Spacebar + j to down arrow",
          "conditions": [ { "name": "spacebar-flag", "type": "variable_if", "value": 1 } ],
          "from": { "key_code": "j" },
          "to": [ { "key_code": "down_arrow" } ]
        },
        {
          "type": "basic",
          "description": "Spacebar + k to up arrow",
          "conditions": [ { "name": "spacebar-flag", "type": "variable_if", "value": 1 } ],
          "from": { "key_code": "k" },
          "to": [ { "key_code": "up_arrow" } ]
        },
        {
          "type": "basic",
          "description": "Spacebar + l to right arrow",
          "conditions": [ { "name": "spacebar-flag", "type": "variable_if", "value": 1 } ],
          "from": { "key_code": "l" },
          "to": [ { "key_code": "right_arrow" } ]
        },
        { "description": "Spacebar Flag",
          "type": "basic",
          "from": {
            "key_code": "spacebar"
          },
          "to": [ { "set_variable": { "name": "spacebar-flag", "value": 1 } } ],
          "to_after_key_up": [ { "set_variable": { "name": "spacebar-flag", "value": 0 } } ],
          "to_if_alone": [ { "key_code": "spacebar" } ]
        }
      ]
    },
    {
      "description": "Tab -> Hyper-Key",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab"
          },
          "parameters": {
            "basic.to_if_held_down_threshold_milliseconds": 100
          },
          "to_if_held_down": [
            {
              "key_code": "right_command",
              "modifiers": [
                "right_shift",
                "left_control",
                "right_option"
              ]
            }
          ],
          "to_if_alone": [
            {
              "key_code": "tab"
            }
          ]
        }
      ]
    },
    {
      "description": "Right Ctrl Mod",
      "manipulators": [
        {
          "type": "basic",
          "description": "Right Control + q to backquart",
          "from": {
            "key_code": "period",
            "modifiers": {
              "mandatory": [
                "right_control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "grave_accent_and_tilde"
            }
          ]
        },
        {
          "type": "basic",
          "description": "Right Control + / to hyphen",
          "from": {
            "key_code": "slash",
            "modifiers": {
              "mandatory": [
                "right_control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "hyphen"
            }
          ]
        }
      ]
    },
    {
      "description": "コマンドキーを単体で押したときに、英数・かなキーを送信する。（左コマンドキーは英数、右コマンドキーはかな） (rev 3)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "parameters": {
            "basic.to_if_held_down_threshold_milliseconds": 100
          },
          "to": [
            {
              "key_code": "left_command",
              "lazy": true
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "left_command"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "japanese_eisuu"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "parameters": {
            "basic.to_if_held_down_threshold_milliseconds": 100
          },
          "to": [
            {
              "key_code": "right_command",
              "lazy": true
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "right_command"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "japanese_kana"
            }
          ]
        }
      ]
    },
    {
      "description": "Cmd + Period to ESC",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "period",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "key_code": "escape"
            }
          ]
        }
      ]
    },
    {
      "description": "Shift + Backspace to DEL",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "delete_or_backspace",
            "modifiers": {
              "mandatory": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "delete_forward"
            }
          ]
        }
      ]
    }
  ]
}
